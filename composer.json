{
  "name" : "stephanieg467/prime_numbers",

  "description" : "Practicing/learning how to use composer",

  "type" : "library",

  "license": "GPL-2.0-or-later",

  "authors": [
    {
      "name": "Stephanie Galata",
      "role": "Student"
    }
  ],

  "require": {
    "monolog/monolog": "1.0.*"
  },

  "config": {
    /*All dependancies will be cloned via git instead of downloaded and extracted*/
    "preferred-install": "source",
    /*If you are working with Composer Patches and preferred-install: source*/
    "discard-changes": true,
    /*sort all your packages inside the composer.json in alphabetical order*/
    "sort-packages": true,
    /*we always fix / force the PHP version in our composer to match the current live system*/
    "platform": {
      "php": "7.1.9"
    }
  }

}
